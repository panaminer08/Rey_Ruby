<%= form_with model: @student do |form| %>
  Firstname: <br> <%= form.text_field :first_name %><br>
  Lastname: <br> <%= form.text_field :last_name %><br>
  Username: <br> <%= form.text_field :username %><br>
  Password: <br> <%= form.password_field :password %><br>
  Level: <br> <%= form.select :highest_completed_education, options_for_select([['High School'], ['Bachelors'], ['MBA'], ['PHD']]) %><br>
  <%= form.submit %>
<% end %>

 